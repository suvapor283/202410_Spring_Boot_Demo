<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.basic.domain.auth.dao.MemberDao">
    <insert id="save" parameterType="com.example.basic.domain.auth.entity.Member">
        INSERT INTO `member`
            SET username = #{username}
                , `password` = #{password}
                , `role` = #{role}
    </insert>
</mapper>